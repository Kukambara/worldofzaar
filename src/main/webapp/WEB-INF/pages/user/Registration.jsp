<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>Registration</title>
	<link rel="stylesheet" type="text/css" href="/resources/css/Registration.css">
	<script type="text/javascript" src="/resources/js/jquery-2.0.3.js"></script>
	<script type="text/javascript" src="/resources/js/Registration.js"></script>
    </head>
    <body>
	
        <div class="registrationForm" id="registrationForm">

		<div class="background">
        	<img src="/resources/Images/Backgrounds/registrationBackground.png"  onselectstart="return false" onmousedown="return false"/>
        </div>

		<div class="ribbon">
        	<img src="/resources/Images/Registration/Ribbons/ribbon.png"  onselectstart="return false" onmousedown="return false"/>
        </div>

        <div class="titleText">
		    <center>
        	    <img src="/resources/Images/Registration/Lables/userCreate.png"  onselectstart="return false" onmousedown="return false"/>
		    </center>
        </div>
		
        <input id="userName"  type="text" class="name" placeholder="Введите имя"/>
        	
		<div class="hideScrollForDescription">
		<textarea id="description" name="description" onselectstart="return false" onmousedown="return false" readonly></textarea>
		</div>
	
		<div class="topLeftArrow" >
        	<img id="topLeftArrow" src="/resources/Images/Registration/NavigationArrows/leftTop.png" onselectstart="return false" onmousedown="return false" />
        	</div>

		<div class="topRightArrow" >
        	<img id="topRightArrow" src="/resources/Images/Registration/NavigationArrows/rightTop.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>
		
		<div class="middleLeftArrow">
        	<img id="middleLeftArrow" src="/resources/Images/Registration/NavigationArrows/leftMiddle.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>
		
		<div class="middleRightArrow">
        	<img id="middleRightArrow" src="/resources/Images/Registration/NavigationArrows/rightMiddle.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="bottomLeftArrow">
        	<img id="bottomLeftArrow" src="/resources/Images/Registration/NavigationArrows/leftBottom.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="bottomRightArrow">
        	<img id="bottomRightArrow" src="/resources/Images/Registration/NavigationArrows/rightBottom.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="sex">
        	<img id="sex"   onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="race">
        	<img id="race"   onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="class">
        	<img id="class"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="avatar">
        	<img id="avatar" onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="leftAvatarArrow">
        	<img id="leftAvatarArrow" src="/resources/Images/Registration/NavigationArrows/leftAvatar.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="rightAvatarArrow">
        	<img id="rightAvatarArrow" src="/resources/Images/Registration/NavigationArrows/rightAvatar.png"  onselectstart="return false" onmousedown="return false"/>
        	</div>
		
		<div class="cloth">
        	<img id="cloth" onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="arms">
        	<img id="arms"  onselectstart="return false" onmousedown="return false"/>
        	</div>

		<div class="startButton" >
        	<img id="startButton" src="/resources/Images/Registration/Lables/start.png"   onselectstart="return false" onmousedown="return false"/>
        	</div>
	</div>
    </body>
</html>